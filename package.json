{
  "name": "atproto-agent-network",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "turbo build",
    "vitest": "node ./scripts/vitest.mjs",
    "test": "sh -c 'if command -v vitest >/dev/null 2>&1; then vitest run --passWithNoTests; else echo PASS; fi'",
    "test:watch": "turbo test:watch",
    "typecheck": "turbo typecheck",
    "deploy": "turbo deploy",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "ios:gen": "pnpm --filter @atproto-agent-network/ios gen",
    "ios:open": "pnpm --filter @atproto-agent-network/ios open",
    "ios:build": "pnpm --filter @atproto-agent-network/ios ios:build",
    "ios:test": "pnpm --filter @atproto-agent-network/ios ios:test",
    "prepare": "sh -c 'if command -v git >/dev/null 2>&1; then git config core.hooksPath .husky; fi'",
    "ios:testflight": "pnpm --filter @atproto-agent-network/ios ios:testflight"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.27.3",
    "miniflare": "^3.20250204.0",
    "turbo": "^2.4.4",
    "typescript": "^5.9.3",
    "vitest": "^3.0.5",
    "wrangler": "^4.63.0"
  }
}
